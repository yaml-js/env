{
  "packageManager": "yarn@4.4.0",
  "name": "@yaml-js/envyaml",
  "description": "Enhance your Vite-powered projects with seamless YAML integration using the Vite YAML Plugin. This lightweight and efficient plugin empowers developers to utilize YAML files directly within their Vite setups.",
  "version": "${FROM TAG}",
  "type": "module",
  "types": "./dist/yaml-js.envyaml.d.ts",
  "main": "./dist/yaml-js.envyaml.js",
  "module": "./dist/yaml-js.envyaml.js",
  "bin": "./dist/cli/main.js",
  "files": [
    "dist",
    "./LICENSE",
    "./README.md"
  ],
  "keywords": [
    "yaml",
    "yml",
    ".env"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yaml-js/env",
    "directory": "packages/library"
  },
  "bugs": {
    "url": "https://github.com/yaml-js/env/issues"
  },
  "homepage": "https://yaml-js.org",
  "exports": {
    ".": {
      "import": "./dist/yaml-js.envyaml.js",
      "require": "./dist/yaml-js.envyaml.cjs",
      "types": "./dist/yaml-js.envyaml.d.ts"
    }
  },
  "scripts": {
    "lint": "eslint -c ./config/eslint.cjs 'src/**/*.ts' 'tests/**/*.ts'",
    "format": "prettier --config ./config/prettier.cjs 'src/**/*.ts' --write",
    "test": "jest --config ./config/jest.ts --collect-coverage --coverage",
    "deploy": "npm publish --access=public",
    "build": "yarn tsup --config ./config/build.ts"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.4.0",
    "@types/yaml": "^1.9.7",
    "@yaml-js/config-eslint": "^0.1.4",
    "@yaml-js/config-prettier": "^0.1.4",
    "@yaml-js/types": "^1.0.1",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "jest-junit": "^16.0.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0",
    "vite": "^5.4.1"
  },
  "dependencies": {
    "yaml": "^2.5.0"
  }
}
